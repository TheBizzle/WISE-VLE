                      <responseDeclaration baseType="string" cardinality="single" identifier="NOTE_RESPONSE_2" />
                      <itemBody>
                        <extendedTextInteraction hasInlineFeedback="false" placeholderText="Eggs and sperm have half the chromosome number because..." responseIdentifier="NOTE_RESPONSE_1" expectedLines="6">
                          <prompt>&lt;!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN"
"http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd"&gt;
&lt;html xmlns="http://www.w3.org/1999/xhtml"&gt;
