                </OTEvidence>
                <OTChallengeQuestion id="4a8536a8-fd53-11dd-8ac9-4b28ce194fa1" title="Challenge Question: Meiosis" authorNotes="" hoverString="Pas Step" speechText="" isInjectPrompt="false" lockStudentAnswers="true" randomChoiceOrder="false" gotoStepIndex="0" maxAttempts="4">
                  <jaxbXML>
                    <assessmentItem xmlns="http://www.imsglobal.org/xsd/imsqti_v2p0" xmlns:ns3="http://www.w3.org/1998/Math/MathML" xmlns:ns2="http://www.w3.org/1999/xlink" timeDependent="false" adaptive="false" identifier="ChallengeQuestion(4134)">
                      <responseDeclaration baseType="identifier" cardinality="single" identifier="PRIMARY_RESPONSE">
                        <correctResponse interpretation="choice2">
                          <value isDefault="false" isCorrect="false">choice2</value>
                        </correctResponse>
