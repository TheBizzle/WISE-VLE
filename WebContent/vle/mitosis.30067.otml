                    <assessmentItem xmlns="http://www.imsglobal.org/xsd/imsqti_v2p0" xmlns:ns3="http://www.w3.org/1998/Math/MathML" xmlns:ns2="http://www.w3.org/1999/xlink" timeDependent="false" adaptive="false" identifier="MasterWorkNote(90934)">
                      <responseDeclaration baseType="string" cardinality="single" identifier="NOTE_RESPONSE_1" />
                      <responseDeclaration baseType="string" cardinality="single" identifier="NOTE_RESPONSE_2" />
                      <responseDeclaration baseType="string" cardinality="single" identifier="NOTE_RESPONSE_3" />
                      <itemBody>
                        <extendedTextInteraction hasInlineFeedback="false" placeholderText="This makes sense because..." responseIdentifier="NOTE_RESPONSE_1" expectedLines="6">
                          <prompt>&lt;!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN"
