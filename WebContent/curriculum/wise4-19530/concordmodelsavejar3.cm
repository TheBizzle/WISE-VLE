<!DOCTYPE HTML PUBLIC "-//W3C//DTD XHTML 1.1 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml"><head>


	<meta http-equiv="Content-Type" content="text/html; charset=ISO-8859-1">
	<title>Airbags Model (Flash)</title>
	<link rel="stylesheet" type="text/css" href="airbags.css">
	<script type="text/javascript" src="swfobject.js"></script>
<style media="screen" type="text/css">#flexApplication {visibility:hidden}</style>
	<script type="text/javascript">
/* Set up parameter selection options */
function setup() {
	var insertForm = "Position:<select name='position' id='position'>";
	for (var i=0; i<26; i++){
		var val = i*.02;
		var text = "<option value='" + val + "'>" + val + "</option>";
		insertForm += text;
	}
	insertForm += "</select><br />Velocity:<select name='velocity' id='velocity'>";
	
	var insertVel = "";
	for (var i=-10; i<1; i++){
		var text = "<option value='" + i + "'>" + i + "</option>";
		insertForm += text;
	}
	insertForm += "</select><br />Time (delay):<select name='time' id='time'>";
	
	var insertTime = "";
	for (var i=0; i<10; i++){
		var val = i*.01;
		var text = "<option value='" + val + "'>" + val + "</option>";
		insertForm += text;
	}
	insertForm += "</select>";
	
	document.getElementById('parameters').innerHTML += insertForm;
}

/* Get user input parameters and send to Flex model */
function setParameters() {
	var position = document.getElementById('position').value;
	var velocity = document.getElementById('velocity').value;
	var time = document.getElementById('time').value;
	getFlexApp('AirbagsModel').newParameters(position, velocity, time);
}

function exportParameters(position, velocity, time) {
	document.getElementById('position').value = position;
	document.getElementById('velocity').value = velocity;
	document.getElementById('time').value = time;
	document.getElementById('submit').disabled = false;
}

function disableParameterButton(){
	document.getElementById('submit').disabled = true;
}

function alertState() {
	getFlexApp('AirbagsModel').showParameters();
}
function showState(position, velocity, time) {
	alert("Dummy position: " + position + " m\r\nDummy velocity: " + velocity + " m\/s\r\nDummy time delay: " + time + " s");
}

function getFlexApp(movieName) {
	if (navigator.appName.indexOf('Microsoft') != -1) {
		return window[movieName];
	}
	else {
		return document[movieName];
	}
}
	
	</script>
	
</head><div FirebugVersion="1.3.3" style="display: none;" id="_firebugConsole"></div><body onload="setup()">

<div id="model">
	<object classid="clsid:d27cdb6e-ae6d-11cf-96b8-444553540000" id="AirbagsModel" height="600" width="650">
	    <param name="movie" value="AirbagsModel.swf">
	    <param name="allowScriptAccess" value="always">
	    <embed pluginspage="http://www.adobe.com/go/getflashplayer" flashvars="" src="AirbagsModel.swf" name="AirbagsModel" height="600" width="650">
	    
